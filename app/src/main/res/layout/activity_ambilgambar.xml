<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F3F1E6"
    tools:context=".ambilgambar">

    <ImageView
        android:id="@+id/hasilfoto"
        android:layout_width="409dp"
        android:layout_height="296dp"
        android:layout_marginTop="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="411dp"
        android:layout_height="0dp"
        android:background="#AACC67"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageButton
            android:id="@+id/btnkembali3"
            android:layout_width="59dp"
            android:layout_height="54dp"
            android:backgroundTint="#00FFFFFF"
            android:contentDescription="kembali"
            android:scaleType="centerCrop"
            app:srcCompat="@drawable/kembali"
            app:tint="#FBFBFA" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="101dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:text="Ambil Gambar"
            android:textColor="#FFFFFF"
            android:typeface="serif" />
    </LinearLayout>

    <Button
        android:id="@+id/btnlabelfoto"
        android:layout_width="177dp"
        android:layout_height="50dp"

        android:layout_marginStart="117dp"
        android:layout_marginEnd="117dp"
        android:layout_marginBottom="52dp"
        android:text="Tambahkan Data"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#070707"
        android:textIsSelectable="false"
        android:typeface="serif"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="409dp"
        android:layout_height="240dp"
        android:layout_marginTop="350dp"
        android:layout_marginBottom="118dp"
        android:background="#DCEDC8"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="344dp">

            <EditText
                android:id="@+id/editTextName2"
                android:layout_width="219dp"
                android:layout_height="54dp"
                android:layout_marginStart="160dp"
                android:layout_marginTop="43dp"
                android:layout_marginBottom="154dp"
                android:backgroundTint="#090808"
                android:ems="10"
                android:hint="Masukkan merk botol"
                android:inputType="text"
                android:textColor="#070707"
                android:textColorHighlight="#020202"
                android:textColorHint="#80070707"
                android:textColorLink="#000000"
                android:textSize="15sp"
                android:typeface="serif"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.706" />

            <TextView
                android:id="@+id/lantai2"
                android:layout_width="107dp"
                android:layout_height="21dp"
                android:layout_marginStart="45dp"
                android:layout_marginTop="114dp"
                android:layout_marginEnd="259dp"
                android:layout_marginBottom="116dp"
                android:shadowColor="#131313"
                android:text="Lantai          :"
                android:textColor="#0B0A0A"
                android:textColorHighlight="#151414"
                android:textColorLink="#100F0F"
                android:textSize="16sp"
                android:textStyle="bold"
                android:typeface="serif"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.706" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="107dp"
                android:layout_height="21dp"
                android:layout_marginStart="45dp"
                android:layout_marginTop="168dp"
                android:layout_marginEnd="259dp"
                android:layout_marginBottom="62dp"
                android:shadowColor="#131313"
                android:text="Tanggal       :"
                android:textColor="#0B0A0A"
                android:textColorHighlight="#151414"
                android:textColorLink="#100F0F"
                android:textSize="16sp"
                android:textStyle="bold"
                android:typeface="serif"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.706" />

            <EditText
                android:id="@+id/editTextDate2"
                android:layout_width="219dp"
                android:layout_height="54dp"
                android:layout_marginStart="160dp"
                android:layout_marginTop="154dp"
                android:layout_marginEnd="32dp"
                android:layout_marginBottom="43dp"
                android:backgroundTint="#090808"
                android:ems="10"
                android:hint="Masukkan Tanggal"
                android:inputType="date"
                android:textColor="#040404"
                android:textColorHighlight="#000000"
                android:textColorHint="#80000000"
                android:textColorLink="#000000"
                android:textSize="15sp"
                android:typeface="serif"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.706" />

            <EditText
                android:id="@+id/editTextLokasi2"
                android:layout_width="219dp"
                android:layout_height="54dp"
                android:layout_marginStart="160dp"
                android:layout_marginTop="98dp"
                android:layout_marginEnd="32dp"
                android:layout_marginBottom="99dp"
                android:backgroundTint="#090808"
                android:ems="10"
                android:hint="Masukkan lokasi lantai"
                android:inputType="text"
                android:textColor="#020202"
                android:textColorHighlight="#070707"
                android:textColorHint="#80000000"
                android:textColorLink="#020202"
                android:textSize="15sp"
                android:typeface="serif"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.706" />

            <TextView
                android:id="@+id/merkbotol"
                android:layout_width="105dp"
                android:layout_height="22dp"
                android:layout_marginStart="45dp"
                android:layout_marginTop="61dp"
                android:layout_marginEnd="261dp"
                android:layout_marginBottom="168dp"
                android:text="Merk Botol :"
                android:textColor="#0C0C0C"
                android:textColorHighlight="#020202"
                android:textColorLink="#131212"
                android:textSize="16sp"
                android:textStyle="bold"
                android:typeface="serif"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.706" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>